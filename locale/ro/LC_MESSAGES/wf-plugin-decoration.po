#. (itstool) path: plugin/_short
msgid "Decoration"
msgstr "Decorație"

#. (itstool) path: plugin/_long
msgid "Default decorations around XWayland windows."
msgstr "Decorațiile implicite în jurul ferestrelor XWayland."

#. (itstool) path: option/_short
msgid "Font"
msgstr "Font"

#. (itstool) path: option/_long
msgid "Sets the font to use for the title bars."
msgstr "Stabilește fontul de folosit pentru barele de titlu."

#. (itstool) path: option/_short
msgid "Font color"
msgstr "Culoarea fontului"

#. (itstool) path: option/_long
msgid "Sets the font color to use for the title bars."
msgstr "Stabilește culoarea fontului de folosit pentru barele de titlu."

#. (itstool) path: option/_short
msgid "Title bar height"
msgstr "Înălțimea barei de titlu"

#. (itstool) path: option/_long
msgid "Sets the height of the title bars in pixels."
msgstr "Stabilește înălțimea barei de titlu, în pixeli."

#. (itstool) path: option/_short
msgid "Border size"
msgstr "Dimensiunea bordurii"

#. (itstool) path: option/_long
msgid "Sets the border size in pixels."
msgstr "Stabilește dimensiunea bordurii, în pixeli."

#. (itstool) path: option/_short
msgid "Order of window buttons"
msgstr "Ordinea butoanelor de fereastră"

#. (itstool) path: option/_long
msgid "Sets the order of the window buttons."
msgstr "Stabilește ordinea butoanelor de fereastră."

#. (itstool) path: option/_short
msgid "Color when window is active"
msgstr "Culoare când fereastra e activă"

#. (itstool) path: option/_long
msgid "Sets the color when the window is active."
msgstr "Stabilește culoarea ramei când fereastra este activă."

#. (itstool) path: option/_short
msgid "Color when window is inactive"
msgstr "Culoare când fereastra e inactivă"

#. (itstool) path: option/_long
msgid "Sets the color when the window is inactive."
msgstr "Stabilește culoarea ramei când fereastra este inactivă."

#. (itstool) path: option/_short
msgid "Decoration disabled for specified window types"
msgstr "Decorația dezactivată pentru tipurile specificate de fereastră"

#. (itstool) path: option/_long
msgid "Disables window decoration for windows matching the specified criteria."
msgstr ""
"Dezactivează decorația pentru ferestrele ce îndeplinesc criteriile "
"specificate."

#. (itstool) path: option/_short
msgid "Force decorations for specified window types"
msgstr "Forțează decorații pentru tipurile specificate de fereastră"

#. (itstool) path: option/_long
msgid ""
"Forces window decoration for windows matching the specified criteria, "
"overriding client decoration preference."
msgstr ""
"Forțează decorația pentru ferestrele ce îndeplinesc criteriile specificate, "
"ignorând preferințele privind decorațiile ale clientului."
